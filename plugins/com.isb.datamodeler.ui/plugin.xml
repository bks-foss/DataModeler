<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="newDataModelerProjectConfigurationPage" name="New DataModeler Project ConfigurationPage" schema="schema/newDataModelerProjectConfigurationPage.exsd"/>
   <extension-point id="dataModelerNavigatorLabelProvider" name="DataModeler Navigator Label Provider" schema="schema/dataModelerNavigatorLabelProvider.exsd"/>
   <extension-point id="importWizards" name="Import Wizards" schema="schema/importWizards.exsd"/>
   <extension-point id="exportWizards" name="Export Wizards" schema="schema/exportWizards.exsd"/>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.isb.datamodeler.internal.ui.perspectives.DataModelerPerspective"
            id="com.isb.datamodeler.internal.ui.perspectives.DataModelerPerspective"
            icon="icons/full/obj16/datamodelerPrespective.png"
            name="%DataModelerPerspective.label">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="com.isb.datamodeler.internal.ui.perspectives.DataModelerPerspective">
         <view
               closeable="true"
               id="com.isb.datamodeler.internal.ui.views.DataModelerView"
               minimized="false"
               moveable="true"
               ratio="0.20"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.ResourceNavigator"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="com.isb.datamodeler.internal.ui.views.DataModelerView"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               moveable="true"
               ratio="0.70"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.ui.views.TaskList"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.ui.views.PropertySheet"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
         <view
               closeable="true"
               id="org.eclipse.pde.runtime.LogView"
               minimized="false"
               moveable="true"
               relationship="stack"
               relative="org.eclipse.ui.views.PropertySheet"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
         <viewShortcut
               id="com.isb.datamodeler.internal.ui.views.DataModelerView">
         </viewShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.isb.datamodeler.category"
            name="%DataModelerViewCategory.label">
      </category>
      <view
            category="com.isb.datamodeler.category"
            class="com.isb.datamodeler.internal.ui.views.DataModelerView"
            id="com.isb.datamodeler.internal.ui.views.DataModelerView"
            icon="icons/full/obj16/datamodelerView.png"
            name="%DataModelerNavigator.label">
      </view>
  	  <view
	  		category="com.isb.datamodeler.category"
	  		class="com.isb.datamodeler.internal.ui.views.DataModelerValidationView"
     		icon="icons/cview/navigator.gif"
	 		id="com.isb.datamodeler.internal.ui.views.DataModelerValidationView"
	  		name="%DataModelerValidationView.name">
	</view>
   </extension>
   
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="com.isb.datamodeler.internal.ui.views.DataModelerView">
         <includes>
            <contentExtension pattern="com.isb.datamodeler.ui.diagram.domainResourceContent">
            </contentExtension>
            <contentExtension
                  pattern="com.isb.datamodeler.ui.navigator.filters.workingSet">
            </contentExtension>
            <contentExtension
                  pattern="com.isb.datamodeler.ui.workingSets">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="com.isb.datamodeler.internal.ui.views.DataModelerView">
         <includes>
            <actionExtension
                  pattern="com.isb.datamodeler.ui.navigator.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewer
            viewerId="com.isb.datamodeler.internal.ui.views.DataModelerView">
         <popupMenu
               allowsPlatformContributions="false"
               id="com.isb.datamodeler.internal.ui.views.DataModelerView#PopupMenu">
            <insertionPoint
                  name="group.new"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.edit"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.reorganize"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.validation"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.port"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="additions"
                  separator="true">
            </insertionPoint>
            <insertionPoint
                  name="group.compare"
                  separator="true">
            </insertionPoint>
         </popupMenu>
      </viewer>
   </extension>
