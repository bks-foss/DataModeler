            id="com.isb.datamodeler.ui.navigator.DataModelerEditActions">
         <enablement>
            <instanceof
                  value="com.isb.datamodeler.diagram.navigator.DatamodelerDomainNavigatorItem">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
            appearsBefore="com.isb.datamodeler.ui.navigator.DataModelerValidationActions"
            class="com.isb.datamodeler.internal.ui.views.actions.refactor.DataModelerRefactorActionProvider"
            id="com.isb.datamodeler.ui.navigator.DataModelerRefactorActions">
         <enablement>
            <instanceof
                  value="com.isb.datamodeler.diagram.navigator.DatamodelerDomainNavigatorItem">
            </instanceof>
         </enablement>
      </actionProvider>
      <actionProvider
