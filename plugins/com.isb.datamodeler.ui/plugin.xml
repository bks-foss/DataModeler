         <enablement>
            <adapt
                  type="com.isb.datamodeler.ui.project.EProject">
            </adapt>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.isb.datamodeler.internal.ui.views.actions.porting.DataModelerPortActionProvider"
            id="com.isb.datamodeler.ui.navigator.DataModelerPortActions">
         <enablement>
            <or>
               <adapt
                     type="com.isb.datamodeler.ui.project.EProject">
               </adapt>
               <adapt
                     type="com.isb.datamodeler.tables.EPersistentTable">
               </adapt>
               <adapt
                     type="com.isb.datamodeler.schema.ESchema">
               </adapt>
            </or>
         </enablement>
      </actionProvider>
      <actionProvider
            class="com.isb.datamodeler.internal.ui.views.actions.DataModelerUndoRedoActionProvider"
            id="com.isb.datamodeler.ui.navigator.DataModelerUndoRedoActions">
         <enablement>
            <or></or>
         </enablement>
      </actionProvider>
   </extension>
   
	
