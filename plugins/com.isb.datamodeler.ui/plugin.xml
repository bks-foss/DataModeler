            labelProvider="com.isb.datamodeler.internal.ui.views.DatamodelerDomainNavigatorLabelProvider"
            activeByDefault="true">
         <enablement>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IResource">
               </instanceof>
            </or>
         </enablement>
         <commonSorter
               class="com.isb.datamodeler.internal.ui.views.DatamodelerCommonSorter"
               id="datamodelerCommonSorter">
            <parentExpression>
               <or>
                  <instanceof
                        value="com.isb.datamodeler.diagram.navigator.DatamodelerDomainNavigatorItem">
