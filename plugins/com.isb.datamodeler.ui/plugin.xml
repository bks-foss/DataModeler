            name="DataModeler WorkingSet Filter">
      </commonFilter>
      <actionProvider
            class="com.isb.datamodeler.internal.ui.views.actions.workingset.DataModelerWorkingSetActionProvider"
            id="com.isb.datamodeler.ui.navigator.WorkingSetActions">
         <enablement>
            <or>
               <adapt
                     type="org.eclipse.core.resources.IResource">
               </adapt>
               <adapt
                     type="java.util.Collection">
                  <count
                        value="0">
                  </count>
               </adapt>
            </or>
