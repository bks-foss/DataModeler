               </instanceof>
               <instanceof
                     value="org.eclipse.ui.IWorkingSet">
               </instanceof></or>
         </enablement>
      </actionProvider>
      <actionProvider
            appearsBefore="com.isb.datamodeler.ui.navigator.DataModelerRefactorActions"
